			filter="STRING"
			description="OBSERVER_ADDRESS_DESC"
			hint="OBSERVER_ADDRESS_PL"
			type="text"/>
	<field name="observer_tel"
			alias="observer_tel"
			class="form-control"
			label="OBSERVER_TEL"
			description="OBSERVER_TEL_DESC"
			hint="OBSERVER_TEL"
			type="tel"/>
	<field name="observer_email"
			alias="observer_email"
			class="form-control"
			label="OBSERVER_EMAIL"
			description="OBSERVER_EMAIL_DESC"
			hint="OBSERVER_EMAIL"
			type="email"/>

	<!--Informant contacts-->
	<field name="informant_name"
			alias="informant_name"
			class="form-control"
			label="INFORMANT_NAME"
			filter="STRING"
			description="INFORMANT_NAME_DESC"
			hint="INFORMANT_NAME_PL"
			type="text"/>
	<field name="informant_address"
			alias="informant_address"
			class="form-control"
			label="INFORMANT_ADDRESS"
			filter="STRING"
			description="INFORMANT_ADDRESS_DESC"
			hint="INFORMANT_ADDRESS_PL"
			type="text"/>
	<field name="informant_tel"
			alias="informant_tel"
			class="form-control"
			label="INFORMANT_TEL"
			description="INFORMANT_TEL_DESC"
			hint="INFORMANT_TEL"
			type="tel"/>
	<field name="informant_email"
			alias="informant_email"
			class="form-control"
			label="INFORMANT_EMAIL"
			description="INFORMANT_EMAIL_DESC"
			hint="INFORMANT_EMAIL"
			type="email"/>

	<!--Date-->
	<field name="observation_datetime"
			alias="observation_datetime"
			class="form-control form-calendar"
			label="OBSERVATION_DATE"
			description="OBSERVATION_DATE_DESC"
			hint="OBSERVATION_DATE"
			type="ckcalendar"
            default="NOW"
            format="Y-m-d"/>

