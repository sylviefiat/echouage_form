<?xml version="1.0" encoding="utf-8"?>
<form>
	<fieldset name="stranding_admin.details"
			label="OBSERVATION_ADMIN_REPORT"
			printable="true"
			addfieldpath="/libraries/jdom/jform/fields">
	<!--Id-->
	<field name="id"
			alias="id"
			class="form-control"
			label="ID"
			filter="INT"
			description="ID_DESC"
			readonly="true"
			type="cktext"/>

	<!--Location_ID-->
	<field name="id_location"
			alias="id_location"
			class="form-control"
			label="ID_LOCATION"
			filter="INT"
			description="ID_LOCATION_DESC"
			readonly="true"
			type="cktext"/>
	
<fieldset name="contacts" label="Contacts">
	<!--Observer contacts-->
	<field name="observer_name"
			alias="observer_name"
			class="form-control"
			label="OBSERVER_NAME"
			filter="STRING"
			description="OBSERVER_NAME_DESC"
			required="true"
			hint="OBSERVER_NAME_PL"
			type="text"/>
	<field name="observer_address"
			alias="observer_address"
			class="form-control"
			label="OBSERVER_ADDRESS"
			filter="STRING"
			description="OBSERVER_ADDRESS_DESC"
			hint="OBSERVER_ADDRESS_PL"
			type="text"/>
	<field name="observer_tel"
			alias="observer_tel"
			class="form-control"
			label="OBSERVER_TEL"
			description="OBSERVER_TEL_DESC"
			hint="OBSERVER_TEL"
			type="tel"/>
	<field name="observer_email"
			alias="observer_email"
			class="form-control"
			label="OBSERVER_EMAIL"
			description="OBSERVER_EMAIL_DESC"
			hint="OBSERVER_EMAIL"
			type="email"/>

	<!--Informant contacts-->
	<field name="informant_name"
			alias="informant_name"
			class="form-control"
			label="INFORMANT_NAME"
			filter="STRING"
			description="INFORMANT_NAME_DESC"
			hint="INFORMANT_NAME_PL"
			type="text"/>
	<field name="informant_address"
			alias="informant_address"
			class="form-control"
			label="INFORMANT_ADDRESS"
			filter="STRING"
			description="INFORMANT_ADDRESS_DESC"
			hint="INFORMANT_ADDRESS_PL"
			type="text"/>
	<field name="informant_tel"
			alias="informant_tel"
			class="form-control"
			label="INFORMANT_TEL"
			description="INFORMANT_TEL_DESC"
			hint="INFORMANT_TEL"
			type="tel"/>
	<field name="informant_email"
			alias="informant_email"
			class="form-control"
			label="INFORMANT_EMAIL"
			description="INFORMANT_EMAIL_DESC"
			hint="INFORMANT_EMAIL"
			type="email"/>
</fieldset>

<fieldset name="circunstances" label="Circunstances">

	<!--Date-->
	<field name="observation_datetime"
			alias="observation_datetime"
			class="form-control form-calendar"
			label="OBSERVATION_DATE"
			description="OBSERVATION_DATE_DESC"
			hint="OBSERVATION_DATE"
			type="ckcalendar"
            default="NOW"
            format="Y-m-d"/>	

	<!--Position-->
	<field name="observation_location"
			alias="observation_location"
			class="form-control"
			label="OBSERVATION_LOCATION"
			filter="STRING"
			description="OBSERVATION_LOCATION_DESC"
			hint="OBSERVATION_LOCATION"
			type="textarea"/>
	<field name="observation_localisation"
			alias="observation_localisation"
			label="OBSERVATION_PICK"
			description="OBSERVATION_PICK_DESC"
			filter="STRING"
			class="formatted_address form-control"
			type="ckaddresspicker"
			required="true"/>
	<field name="observation_region"
			alias="region"
			label="OBSERVATION_REGION"
			description="OBSERVATION_REGION_DESC"
			filter="STRING"
			class="region form-control"
			hint="OBSERVATION_REGION"
			type="text"/>
	<field name="observation_country"
			alias="country"
			label="OBSERVATION_COUNTRY"
			description="OBSERVATION_COUNTRY_DESC"
			filter="STRING"
			class="country form-control"
			hint="OBSERVATION_COUNTRY"
			type="text"/>
	<field name="observation_country_code"
			readonly="true"
			alias="country_code"
			label="OBSERVATION_COUNTRY_CODE"
			description="OBSERVATION_COUNTRY_CODE_DESC"
			filter="STRING"
			class="country_code form-control"
			hint="OBSERVATION_COUNTRY_CODE"
			type="text"/>	
					
	<field name="observation_latitude"
			alias="latitude"
			label="OBSERVATION_LATITUDE"
			description="OBSERVATION_LATITUDE_DESC"
			filter="FLOAT"
			class="latitude form-control"
			hint="OBSERVATION_LATITUDE"
			type="hidden"/>
	<field name="observation_latitude_dmd"
			alias="latitude_dmd"
			label="OBSERVATION_LATITUDE_DMD"
			description="OBSERVATION_LATITUDE_DMD_DESC"
			filter="FLOAT"
			class="latitude_dmd form-control"
			hint="OBSERVATION_LATITUDE"
			type="text"/>
	<field name="observation_longitude"
			alias="longitude"
			label="OBSERVATION_LONGITUDE"
			description="OBSERVATION_LONGITUDE_DESC"
			hint="OBSERVATION_LONGITUDE"
			filter="FLOAT"
			class="longitude form-control"
			type="hidden"/>
	<field name="observation_longitude_dmd"
			alias="longitude_dmd"
			label="OBSERVATION_LONGITUDE_DMD"
			description="OBSERVATION_LONGITUDE_DMD_DESC"
			hint="OBSERVATION_LONGITUDE"
			filter="FLOAT"
			class="longitude_dmd form-control"
			type="text"/>

	<!--Stranding type-->
	<field 	name="observation_stranding_type"
			alias="observation_stranding_type"
			class="custom-control custom-radio"
			label="OBSERVATION_STRANDING_TYPE"
			description="OBSERVATION_STRANDING_TYPE_DESC"
			type="radio"
			default="isolé"
			required="true" >
			<option value="isolé">OBSERVATION_STRANDING_TYPE_A</option>
			<option value="en groupe">OBSERVATION_STRANDING_TYPE_B</option>
	</field>

	<!--Number-->
	<field name="observation_number"
			alias="observation_number"
			class="form-control"
			label="OBSERVATION_NUMBER"
			description="OBSERVATION_NUMBER_DESC"
			hint="OBSERVATION_NUMBER"
			labelclass="control-label"
			type="number"
			showon="observation_stranding_type!:isolé"
			min="1"
			max="1000"
			disabled="true"/>
</fieldset>

<field
	name="animal_form"
	type="subform"
	label="COM_STRANDING_FORMS_EDIT_ITEM_ANIMAL_DETAILS"
	description="Animal Fields"
	layout="field/subform/repeatable"
	validate="subform"
	multiple="true"
	groupByFieldset="true"
	min="1"
	max="50"
	>
	<form>
	<fieldset name="identification" label="OBSERVATION_ANIMAL_IDENTIFICATION">
		<!--Observation_ID-->
		<field name="observation_id"
			alias="observation_id"
			label="ID_OBSERVATION"
			hiddenLabel="true"
			filter="INT"
			description="ID_OBSERVATION_DESC"
			readonly="true"
			type="hidden"/>

	<!--Species common name-->
	 	<field name="observation_species_common_name" 
	 		alias="observation_species_common_name" 
			label="OBSERVATION_SPECIES_COMMON_NAME_LABEL" 
			description="OBSERVATION_SPECIES_COMMON_NAME_DESC"
			hint="OBSERVATION_SPECIES_COMMON_NAME"
			class="custom-control"
			labelclass="col-lg-6 col-md-6 col-xs-12" 
			trigger="true" 
			content-type="choices"
			type="groupedlist" 
			target="observation_species_genus"
			required="true">
			 <option value="">SPECIES_COMMON_NAME_0</option>
			 <option value="inconnu">SPECIES_UNKNOWN</option>
			 <group label="SPECIES_COMMON_NAME_CETACES_ODONTOCETES">
				 <option value="Cachalot">SPECIES_COMMON_NAME_1</option>
				 <option value="Cachalot pygmée">SPECIES_COMMON_NAME_2</option>
				 <option value="Cachalot nain">SPECIES_COMMON_NAME_3</option>
				 <option value="Baleine à bec de Blainville">SPECIES_COMMON_NAME_4</option>
				 <option value="Baleine à bec de longman">SPECIES_COMMON_NAME_5</option>
				 <option value="Baleine à bec de Cuvier">SPECIES_COMMON_NAME_6</option>
			 	 <option value="Orque">SPECIES_COMMON_NAME_7</option>
				 <option value="Fausse orque">SPECIES_COMMON_NAME_8</option>
				 <option value="Globicéphale tropical">SPECIES_COMMON_NAME_9</option>
				 <option value="Dauphin de Risso">SPECIES_COMMON_NAME_10</option>
				 <option value="Orque Pygmée">SPECIES_COMMON_NAME_11</option>
				 <option value="Péponocéphale ou dauphin d’Electre">SPECIES_COMMON_NAME_12</option>
				 <option value="Sténo ou dauphin à bec étroit">SPECIES_COMMON_NAME_13</option>
				 <option value="Grand dauphin commun">SPECIES_COMMON_NAME_14</option>
				 <option value="Grand dauphin de l’Indo-Pacifique">SPECIES_COMMON_NAME_15</option>
				 <option value="Dauphin commun">SPECIES_COMMON_NAME_16</option>
				 <option value="Dauphin à long bec">SPECIES_COMMON_NAME_17</option>
				 <option value="Dauphin tacheté pantropical">SPECIES_COMMON_NAME_18</option>
				 <option value="Dauphin de Fraser">SPECIES_COMMON_NAME_19</option>
			 </group>
			 <group label="SPECIES_COMMON_NAME_CETACE_MYSTICETES">
				 <option value="Baleine bleue pygmée">SPECIES_COMMON_NAME_20</option>
				 <option value="Rorqual commun">SPECIES_COMMON_NAME_21</option>
				 <option value="Rorqual boréal ou rorqual de Rudolphi">SPECIES_COMMON_NAME_22</option>
				 <option value="Rorqual tropical ou rorqual de Bryde">SPECIES_COMMON_NAME_23</option>
				 <option value="Rorqual de Omura">SPECIES_COMMON_NAME_24</option>
				 <option value="Petit rorqual antarctique">SPECIES_COMMON_NAME_25</option>
				 <option value="Petit rorqual pygmée">SPECIES_COMMON_NAME_26</option>
				 <option value="Baleine à bosse">SPECIES_COMMON_NAME_27</option>
			</group>
			<group label="SPECIES_COMMON_NAME_DUGONG">
			 	 <option value="Dugong ou vache marine">SPECIES_COMMON_NAME_28</option>
			</group>
			<group label="SPECIES_COMMON_NAME_SEA_LION">
				<option value="Otarie à fourrure de Nouvelle-Zélande">SPECIES_COMMON_NAME_29</option>
			</group>	
		</field>

		<!--Species genus-->
		<field name="observation_species_genus"
			alias="observation_species_genus"
			class="custom-control"
			labelclass="col-lg-3 col-md-3 col-xs-3"
			label="OBSERVATION_SPECIES_GENUS_LABEL"
			filter="STRING"
			description="OBSERVATION_SPECIES_GENUS_DESC"
			hint="OBSERVATION_SPECIES_GENUS"
			readonly="true"
			type="text"/>

		<!--Species species-->
		<field name="observation_species"
			alias="observation_species"
			class="custom-control"
			labelclass="col-lg-3 col-md-3 col-xs-3"
			label="OBSERVATION_SPECIES_LABEL"
			filter="STRING"
			description="OBSERVATION_SPECIES_DESC"
			hint="OBSERVATION_SPECIES"
			readonly="true"
			type="text"/>

		<!--Species identification-->
		<field name="observation_species_identification"
			alias="observation_species_identification"
			class="btn-group custom-control"
			labelclass="col-lg-6 col-md-6 col-xs-12"
			label="OBSERVATION_SPECIES_IDENTIFICATION"
			description="OBSERVATION_SPECIES_IDENTIFICATION_DESC"
			type="radio">
			<option value="probable">OBSERVATION_SPECIES_IDENTIFICATION_A</option>
			<option value="certain">OBSERVATION_SPECIES_IDENTIFICATION_B</option>
			<option value="inconnu">OBSERVATION_SPECIES_IDENTIFICATION_C</option>
		</field>		

		<!--Color-->
		<field name="observation_color"
			alias="observation_color"
			class="custom-control"
			labelclass="col-lg-6 col-md-6 col-xs-12"
			label="OBSERVATION_COLOR"
			description="OBSERVATION_COLOR_DESC"
			hint="OBSERVATION_COLOR"
			type="text"/>

		<!--Sex-->
		<field name="observation_sex"
			alias="observation_sex"
			class="btn-group custom-control"
			labelclass="col-lg-6 col-md-6 col-xs-12"
			label="OBSERVATION_SEX"
			description="OBSERVATION_SEX_DESC"
			type="radio"
			required="true" >
			<option value="male">OBSERVATION_SEX_A</option>
			<option value="femelle">OBSERVATION_SEX_B</option>
			<option value="inconnu">OBSERVATION_SEX_C</option>
		</field>

		<!--Encoche médiane à la caudale-->
		<field name="observation_caudal"
			alias="observation_caudal"
			class="custom-control"
			labelclass="col-lg-6 col-md-6 col-xs-12"
			label="OBSERVATION_CAUDAL"
			description="OBSERVATION_CAUDAL_DESC"
			type="radio"
			required="true" >
			<option value="presente">OBSERVATION_CAUDAL_A</option>
			<option value="absente">OBSERVATION_CAUDAL_B</option>
			<option value="absente">OBSERVATION_CAUDAL_C</option>
		</field>

		<!--Beak or Furrows-->
		<field name="observation_beak_or_furrows"
			alias="observation_beak_or_furrows"
			class="btn-group custom-control"
			labelclass="col-lg-12 col-md-12 col-xs-12"
			label="OBSERVATION_BEAK_OR_FURROWS"
			description="OBSERVATION_BEAK_OR_FURROWS_DESC"
			type="checkboxes"
			multiple="true">
			<option value="bec">OBSERVATION_BEAK</option>
			<option value="sillons">OBSERVATION_FURROWS</option>
		</field>

		<!--Tooth, Baleen or Defenses-->
		<field name="observation_tooth_or_baleen_or_defenses"
			alias="observation_tooth_or_baleen_or_defenses"
			class="btn-group custom-control"
			labelclass="col-lg-12 col-md-12 col-xs-12"
			label="OBSERVATION_TOOTH_OR_BALEEN_OR_DEFENSES"
			description="OBSERVATION_TOOTH_OR_BALEEN_OR_DEFENSES_DESC"
			type="radio"
			required="true">
			<option value="dents" >OBSERVATION_TOOTH</option>
			<option value="baleen" >OBSERVATION_BALEEN</option>
			<option value="defenses" >OBSERVATION_DEFENSES</option>
		</field>

		<!--Tooth-->			
			<field name="nb_teeth_upper_left"
				 alias="nb_teeth_upper_left"
				 label="OBSERVATION_UPPER_LEFT"
				 description="OBSERVATION_UPPER_LEFT_DESC"
				 hint="OBSERVATION_UPPER_LEFT"
				 labelclass="col-lg-6 col-md-6 col-xs-12"
				 type="number"
				 max="1000"
				 showon="observation_tooth_or_baleen_or_defenses:dents"
			/>
			<field name="nb_teeth_upper_right"
				 alias="nb_teeth_upper_right"				 
				 label="OBSERVATION_UPPER_RIGHT"
				 description="OBSERVATION_UPPER_RIGHT_DESC"
				 hint="OBSERVATION_UPPER_RIGHT"
				 labelclass="col-lg-6 col-md-6 col-xs-12"
				 type="number"
				 max="1000"
				 showon="observation_tooth_or_baleen_or_defenses:dents"
			/>
			<field name="nb_teeth_lower_left"
				 alias="nb_teeth_lower_left"
				 label="OBSERVATION_LOWER_LEFT"
				 description="OBSERVATION_LOWER_LEFT_DESC"
				 hint="OBSERVATION_LOWER_LEFT"
				 labelclass="col-lg-6 col-md-6 col-xs-12"
				 type="number"
				 max="1000"
				 showon="observation_tooth_or_baleen_or_defenses:dents"
			/>
			<field name="nb_teeth_lower_right"
				 alias="nb_teeth_lower_right"
				 label="OBSERVATION_LOWER_RIGHT"
				 description="OBSERVATION_LOWER_RIGHT_DESC"
				 hint="OBSERVATION_LOWER_RIGHT"
				 labelclass="col-lg-6 col-md-6 col-xs-12"
				 type="number"
				 max="1000"
				 showon="observation_tooth_or_baleen_or_defenses:dents"
			/>
			<field name="observation_teeth_base_diametre"
				alias="observation_teeth_base_diametre"
				label="OBSERVATION_BASE_DIAMETRE_LBL"
				labelclass="col-lg-12 col-md-12 col-xs-12"
				description="OBSERVATION_BASE_DIAMETRE_DESC"
				hint="OBSERVATION_BASE_DIAMETRE_LBL"
				type="number"
				step="0.01"
				max="1000"
				showon="observation_tooth_or_baleen_or_defenses:dents"
			/>

		<!--Baleen-->
		<field name="observation_baleen_color"
			alias="observation_baleen_color"
			labelclass="col-lg-12 col-md-12 col-xs-12"
			label="OBSERVATION_BALEEN_COLOR"
			description="OBSERVATION_BALEEN_COLOR_DESC"
			hint="OBSERVATION_BALEEN_COLOR"
			type="text"
			showon="observation_tooth_or_baleen_or_defenses:baleen"
			/>
		<field name="observation_baleen_height"
			alias="observation_baleen_height"
			label="OBSERVATION_BALEEN_HEIGHT"
			labelclass="col-lg-6 col-md-6 col-xs-12"
			description="OBSERVATION_BALEEN_HEIGHT_DESC"
			hint="OBSERVATION_BALEEN_HEIGHT"
			type="number"
			step="0.01"
			max="1000"
			showon="observation_tooth_or_baleen_or_defenses:baleen"
			/>
		<field name="observation_baleen_base_height"
			alias="observation_baleen_base_height"
			label="OBSERVATION_BALEEN_BASE_DIAMETRE"
			labelclass="col-lg-6 col-md-6 col-xs-12"
			description="OBSERVATION_BALEEN_BASE_DIAMETRE_DESC"
			hint="OBSERVATION_BALEEN_BASE_DIAMETRE"
			type="number"
			step="0.01"
			max="1000"
			showon="observation_tooth_or_baleen_or_defenses:baleen"
			/>
	</fieldset>

	<fieldset name="animal" label="OBSERVATION_ANIMAL_STATE">

		<!--Size-->
		<field name="observation_size"
			alias="observation_size"
			label="OBSERVATION_SIZE"
			labelclass="col-lg-6 col-md-6 col-xs-12"
			description="OBSERVATION_SIZE_DESC"
			hint="OBSERVATION_SIZE"
			type="number"
			step="0.01"
			min="0.00"
			max="10000000.00"
			required="true"/>

		<!--Size precision-->
		<field name="observation_size_precision"
			alias="observation_size_precision"
			class="btn-group custom-control"
			label="OBSERVATION_SIZE_PRECISION"
			labelclass="col-lg-6 col-md-6 col-xs-12"
			description="OBSERVATION_SIZE_PRECISION_DESC"
			type="radio"
			required="true">
			<option value="précise">OBSERVATION_SIZE_PRECISION_A</option>
			<option value="approximative">OBSERVATION_SIZE_PRECISION_B</option>
		</field>

		<!--Abnormalities-->
		<field name="observation_abnormalities"
			alias="observation_abnormalities"
			class="btn-group custom-control"
			label="OBSERVATION_ABNORMALITIES"
			labelclass="col-lg-12 col-md-12 col-xs-12"
			description="OBSERVATION_ABNORMALITIES_DESC"
			type="radio"
			required="true">
			<option value="oui">OBSERVATION_ABNORMALITIES_A</option>
			<option value="non">OBSERVATION_ABNORMALITIES_B</option>
			<option value="inconnu">OBSERVATION_ABNORMALITIES_C</option>
		</field>

		<!--Capture traces-->
		<field name="observation_capture_traces"
			alias="observation_capture_traces"
			class="btn-group custom-control"
			label="OBSERVATION_CAPTURE_TRACES"
			labelclass="col-lg-6 col-md-6 col-xs-12"
			description="OBSERVATION_CAPTURE_TRACES_DESC"
			type="radio"
			required="true">
			<option value="probable">OBSERVATION_CAPTURE_TRACES_A</option>
			<option value="certaine">OBSERVATION_CAPTURE_TRACES_B</option>
			<option value="inconnu">OBSERVATION_CAPTURE_TRACES_C</option>
		</field>

		<!--Catch indices-->
		<field name="catch_indices"
			alias="catch_indices"
			class="form-control custom-control"
			label="CATCH_INDICES"
			labelclass="col-lg-6 col-md-6 col-xs-12"
			filter="STRING"
			description="CATCH_INDICES_DESC"
			hint="CATCH_INDICES_PL"
			type="textarea"
		/>


		<!--Dead or alive-->
		<field name="observation_dead_or_alive"
			alias="observation_dead_or_alive"
			class="btn-group custom-control"
			label="OBSERVATION_DEAD_OR_ALIVE"
			labelclass="col-lg-12 col-md-12 col-xs-12"
			description="OBSERVATION_DEAD_OR_ALIVE_DESC"
			type="radio"
			required="true">
			<option value="mort" >OBSERVATION_DEAD_OR_ALIVE_A</option>
			<option value="vivant" >OBSERVATION_DEAD_OR_ALIVE_B</option>
		</field>

		<!--Death-->
		<field name="observation_death"
			alias="observation_death"
			class="btn-group custom-control"
			label="OBSERVATION_DEATH"
			labelclass="col-lg-12 col-md-12 col-xs-12"
			description="OBSERVATION_DEATH_DESC"
			type="radio"
			showon="observation_dead_or_alive:mort">
			<option value="mort avant l'arrivée de l'observateur">OBSERVATION_DEATH_A</option>
			<option value="mort pendant l'intervention de l'observateur">OBSERVATION_DEATH_B</option>
			<option value="euthanasié">OBSERVATION_DEATH_C</option>
		</field>

		<!--Date death ckdatetimepicker ou ckcalendar-->
		<field name="observation_datetime_death"
			alias="observation_datetime_death"
			class="form-control form-calendar"
			label="OBSERVATION_DATE_DEATH"
			labelclass="col-lg-4 col-md-4 col-xs-12"
			showon="observation_dead_or_alive:mort"
			description="OBSERVATION_DATE_DEATH_DESC"
			hint="OBSERVATION_DATE_DEATH"
			type="ckdatetimepicker"
			autoclose="true"
			startDate="today"
			uiFormat="d F Y H:i:s"
			format="Y-m-d H:i:s"/>

		<!--State decomposition-->
		<field name="observation_state_decomposition"
			alias="observation_state_decomposition"
			class="btn-group custom-control"
