	<!--Position-->
	<field name="observation_location"
			alias="observation_location"
			class="form-control"
			label="OBSERVATION_LOCATION"
			filter="STRING"
			description="OBSERVATION_LOCATION_DESC"
			hint="OBSERVATION_LOCATION"
