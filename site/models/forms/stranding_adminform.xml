<?xml version="1.0" encoding="utf-8"?>
<form>
	<fieldset name="stranding_admin.details"
			label="OBSERVATION_ADMIN_REPORT"
			printable="true"
			addfieldpath="/libraries/jdom/jform/fields">
	<!--Id-->
	<field name="id"
			alias="id"
			class="form-control"
			label="ID"
			filter="INT"
			description="ID_DESC"
			readonly="true"
			type="cktext"/>

	<!--Location_ID-->
	<field name="id_location"
			alias="id_location"
			class="form-control"
			label="ID_LOCATION"
			filter="INT"
			description="ID_LOCATION_DESC"
			readonly="true"
			type="cktext"/>

	<!--Observation_ID-->
	<field name="id_observation"
			alias="id_observation"
			class="form-control"
			label="ID_OBSERVATION"
			filter="INT"
			description="ID_OBSERVATION_DESC"
			readonly="true"
			type="cktext"/>

	<!--Observer contacts-->
	<field name="observer_name"
			alias="observer_name"
			class="form-control"
			label="OBSERVER_NAME"
