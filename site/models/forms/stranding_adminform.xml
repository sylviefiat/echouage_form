			 class="form-control"
			 label="OBSERVATION_LOWER_LEFT"
			 description="OBSERVATION_LOWER_LEFT_DESC"
			 hint="OBSERVATION_LOWER_LEFT"
			 labelclass="control-label"
			 type="number"
			 max="1000"/>
	<field name="observation_teeth_base_diametre"
			alias="observation_teeth_base_diametre"
			class="form-control"
			label="OBSERVATION_BASE_DIAMETRE_LBL"
			labelclass="control-label"
			description="OBSERVATION_BASE_DIAMETRE_DESC"
			hint="OBSERVATION_BASE_DIAMETRE_LBL"
			type="number"
			step="0.01"
			max="1000"/>

	<!--Baleen-->
	<field name="observation_baleen_color"
			alias="observation_baleen_color"
			class="form-control"
			label="OBSERVATION_BALEEN_COLOR"
			labelclass="control-label"
			description="OBSERVATION_BALEEN_COLOR_DESC"
			hint="OBSERVATION_BALEEN_COLOR"
			type="text"/>
	<field name="observation_baleen_height"
			alias="observation_baleen_height"
			class="form-control"
			label="OBSERVATION_BALEEN_HEIGHT"
			labelclass="control-label"
			description="OBSERVATION_BALEEN_HEIGHT_DESC"
			hint="OBSERVATION_BALEEN_HEIGHT"
			type="number"
			step="0.01"
			max="1000"/>
	<field name="observation_baleen_base_height"
			alias="observation_baleen_base_height"
			class="form-control"
			label="OBSERVATION_BALEEN_BASE_DIAMETRE"
			labelclass="control-label"
			description="OBSERVATION_BALEEN_BASE_DIAMETRE_DESC"
			hint="OBSERVATION_BALEEN_BASE_DIAMETRE"
			type="number"
			step="0.01"
			max="1000"/>

	<!--Size-->
	<field name="observation_size"
			alias="observation_size"
			class="form-control"
			label="OBSERVATION_SIZE"
			labelclass="control-label"
			description="OBSERVATION_SIZE_DESC"
			hint="OBSERVATION_SIZE"
			type="number"
			step="0.01"
			min="0.00"
			max="10000000.00"
			required="true"/>

	<!--Size precision-->
	<field name="observation_size_precision"
			alias="observation_size_precision"
			class="btn-group"
			label="OBSERVATION_SIZE_PRECISION"
			labelclass="control-label"
			description="OBSERVATION_SIZE_PRECISION_DESC"
			type="radio"
			required="true">
			<option value="1">OBSERVATION_SIZE_PRECISION_A</option>
			<option value="2">OBSERVATION_SIZE_PRECISION_B</option>
	</field>

	<!--Abnormalities-->
	<field name="observation_abnormalities"
			alias="observation_abnormalities"
			class="btn-group btn-group-yesno"
			label="OBSERVATION_ABNORMALITIES"
			description="OBSERVATION_ABNORMALITIES_DESC"
			type="radio"
			required="true">
			<option value="oui">OBSERVATION_ABNORMALITIES_A</option>
			<option value="non">OBSERVATION_ABNORMALITIES_B</option>
			<option value="inconnu">OBSERVATION_ABNORMALITIES_C</option>
